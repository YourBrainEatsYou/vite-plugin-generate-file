{
  "name": "vite-plugin-generate-file",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Generate extra file to dist folder.",
  "keywords": [
    "vite",
    "vue",
    "plugin",
    "generate",
    "vite-plugin"
  ],
  "author": "SumyGG Sun <sumyggsun@gmail.com>",
  "homepage": "https://github.com/sumy7/vite-plugin-generate-file#readme",
  "bugs": {
    "url": "https://github.com/sumy7/vite-plugin-generate-file/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumy7/vite-plugin-generate-file.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\"",
    "lint:prettier": "prettier \"{src,test}/**/*.ts\" --check",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "ejs": "^3.1.9",
    "js-yaml": "^4.1.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.9.0",
    "@types/ejs": "^3.1.0",
    "@types/js-yaml": "^4.0.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0"
  }
}
